function getLoadSlidesByCustom() {
    $.cordys.ajax({
        method: "GenerateDynamicPptxReport",
        namespace: "http://schemas.cordys.com/PCRDB_Metadata",
        parameters: {
        	reportName: "validationConcernDescription",	
        	Login_Id: loginID,
        	Project_Code:Project_Code, 
        	prototype:ProtoType,
        },
        async: false,
        dataType: '* json',
        success: function(response) {
        	window.open(window.location.href.match(/[^:]+:[/\\]+[^/\\]+([/\\][^/\\]+[/\\][^/\\]+[/\\])/)[0]+response.tuple.old.generateDynamicPptxReport.generateDynamicPptxReport);
        	window.top.close();
            //window.close();
        }
    });
}

async function initialize() {
	$scope.user = await users.getActiveUser();
	$scope.$apply();
	$scope.messages = await conversations.getMessagesFor($scope.user);
	$scope.$apply();
}


